         CASE META_KEY EQ KS_END         ; CRT 'Shift+End'
         CASE META_KEY EQ KS_ENTER       ; CRT 'Shift+Enter'
         CASE META_KEY EQ KS_ESCAPE      ; CRT 'Shift+Escape'
